<%- include("../header"); %>
<div class="col-12">
    <h1>Agregar usuario</h1>
    <form method="post" action="/usuarios/insertar">
        <div class="form-group">
            <label for="nombre">Nombre</label>
            <input required id="nombre" placeholder="Nombre del usuario" class="form-control" type="text"
                name="nombre">
        </div>
        <div class="form-group">
            <label for="Email">Email</label>
            <input required id="Email" placeholder="Email del usuario" class="form-control" type="text"
                name="Email">
        </div>
        <div class="form-group">
            <label for="Genero">Genero</label>
            <input required id="Genero" placeholder="Genero del usuario" class="form-control" type="text"
                name="Genero">
        </div>
        <div class="form-group">
            <label for="Contraseña">Contraseña</label>
            <input required id="Contraseña" placeholder="Contraseña del usuario" class="form-control" type="text"
                name="Contraseña">
        </div>
        <div class="form-group">
            <button class="btn btn-success">Guardar</button>
            &nbsp;<a href="/usuarios" class="btn btn-primary">Volver</a>
        </div>
    </form>
</div>
<%- include("../footer"); %>